# Backend runtime
API_PREFIX=/api
SQLITE_PATH=data/goodle.db
CORS_ORIGINS=http://localhost:5173

# Gemini
GEMINI_API_KEY=your_gemini_key_here
GEMINI_IMAGE_MODEL=gemini-2.5-flash
GEMINI_VIDEO_MODEL=gemini-2.5-flash

# Temperatures
PHOTO_AI_TEMPERATURE=0.3
VIDEO_AI_TEMPERATURE=0.3
MATCH_AI_TEMPERATURE=0.2

# Matching strategy
MATCH_SIMILARITY_THRESHOLD=70
MATCH_MAX_DISTANCE_KM=5
MATCH_MAX_TIME_GAP_HOURS=72

# Local testing without Gemini
# AI_MOCK_MODE=1
